  "version": "0.2.0",
  "configurations": [
      {
          "type": "pwa-node",
          "cwd": "${workspaceFolder}",
          "request": "launch",
          "name": "DEBUG DEV",
          "runtimeExecutable": "npm",
          "outputCapture": "std",
          "sourceMaps": true,
          "args": [
              "run",
              "dev"
          ],
          "env": {
              "OVERRIDE_ENV_FILE": ".env",
              "TS_NODE_TRANSPILE_ONLY": "true"
          }
      },
  ]
}
